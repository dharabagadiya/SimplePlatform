@model DataModel.Modal.Event
@{ ViewBag.Title = "Event Detail"; }

<div class="row">
    <div class="col-lg-12">
        <div id="page-header" class="clearfix">
            <div class="page-header">
                <h2 class="pull-left">@Model.Name</h2>
                <input type="hidden" id="txtEventID" value="@Model.EventId" />
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="panel panel-default toggle panelMove panelClose panelRefresh">
                <div class="panel-heading">
                    <h4 class="panel-title">Audience List</h4>
                </div>
                <div class="panel-body">
                    <table id="audienceList" class="display">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Contact</th>
                                <th>Visit Date</th>
                                <th>Convention Name</th>
                                <th>FSM Name</th>
                                <th>Booking Status</th>
                                <th>GSM (in $)</th>
                                <th>Donation (in $)</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

